{
  "name": "dd-sdk-reactnative",
  "version": "0.1.1",
  "description": "A client-side React Native module to interact with Datadog",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "dd-sdk-reactnative.podspec",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "prepare": "bob build",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "datadog",
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/DataDog/dd-sdk-reactnative",
  "author": "Datadog (https://github.com/DataDog)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/DataDog/dd-sdk-reactnative/issues"
  },
  "homepage": "https://github.com/DataDog/dd-sdk-reactnative#readme",
  "devDependencies": {
    "@react-native-community/bob": "^0.17.1",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "@types/react-native": "0.63.42",
    "jest": "^26.6.3",
    "pod-install": "^0.1.14",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
